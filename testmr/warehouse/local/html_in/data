<ins class=""adsbygoogle"" style=""display: inline-block; height: 250px; width: 300px; "" data-ad-client=""ca-pub-0283370460932900"" data-ad-slot=""6387932204"" data-adsbygoogle-status=""done""><ins id=""aswift_2_expand"" style=""display:inline-table;border:none;height:250px;margin:0;padding:0;position:relative;visibility:visible;width:300px;background-color:transparent""><ins id=""aswift_2_anchor"" style=""display:block;border:none;height:250px;margin:0;padding:0;position:relative;visibility:visible;width:300px;background-color:transparent""><iframe width=""300"" height=""250"" frameborder=""0"" marginwidth=""0"" marginheight=""0"" vspace=""0"" hspace=""0"" allowtransparency=""true"" scrolling=""no"" allowfullscreen=""true"" onload=""var i=this.id,s=window.google_iframe_oncopy,H=s&amp;&amp;s.handlers,h=H&amp;&amp;H[i],w=this.contentWindow,d;try{d=w.document}catch(e){}if(h&amp;&amp;d&amp;&amp;(!d.body||!d.body.firstChild)){if(h.call){setTimeout(h,0)}else if(h.match){try{h=s.upd(h,i)}catch(e){}w.location.replace(h)}}"" id=""aswift_2"" name=""aswift_2"" style=""left:0;position:absolute;top:0;""></iframe></ins></ins></ins>
<script>
(adsbygoogle = window.adsbygoogle || []).push({});
