function AutoScroll2(obj){  
        $(obj).find(""ul:first"").animate({  
                marginTop:""-24px""  
        },1000,function(){  
                $(this).css({marginTop:""0px""}).find(""li:first"").appendTo(this);  
        });  
}  
$(document).ready(function(){  
setInterval('AutoScroll2(""#scrollDiv1"")',5000)  
});  
</SCRIPT>

